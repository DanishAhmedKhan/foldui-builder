{
    "name": "foldui-builder",
    "version": "0.0.1",
    "type": "module",
    "description": "A programmatic builder for FoldUI documents.",
    "author": "Danish Ahmed Khan",
    "license": "MIT",
    "main": "./dist/foldui-builder.umd.cjs",
    "module": "./dist/foldui-builder.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/foldui-builder.js",
            "require": "./dist/foldui-builder.umd.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "vite",
        "build": "vite build && tsc"
    },
    "keywords": [
        "foldui",
        "ui",
        "ui-builder",
        "schema-builder",
        "document-builder",
        "ui-schema"
    ],
    "devDependencies": {
        "typescript": "~5.9.3",
        "vite": "^7.2.4"
    },
    "peerDependencies": {
        "foldui": "^0.0.2"
    }
}